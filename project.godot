; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="dice-roll"
run/main_scene="res://scenes/menus/MainMenu.tscn"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameplayTransitionManager="*res://scenes/GameplayTransitionManager.tscn"
MenuTransitionManager="*res://scenes/MenuTransitionManager.tscn"
PlaygroundTransitionManager="*res://scenes/PlaygroundTransitionManager.tscn"

[display]

window/size/viewport_width=1024
window/size/viewport_height=768
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="dice-roll"

[editor]

version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray()

[rendering]

textures/canvas_textures/default_texture_filter=0
environment/autoload_on_startup=false
